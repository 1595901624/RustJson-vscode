{
  "name": "rustjson",
  "displayName": "RustJson",
  "description": "A Json to Rust Struct VS Code Plugin",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/1595901624/RustJson-vscode"
  },
  "publisher": "cloris",
  "icon": "logo/rustjson128.png",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "rustjson.Rust2Json",
          "when": "editorFocus",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "rustjson.Rust2Json",
        "title": "RustJson"
      }
    ]
  },
  "scripts": {
    "build-wasm": "cd RustJsonLib && wasm-pack build --target nodejs",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "@wasm-tool/wasm-pack-plugin": "1.5.0",
    "copy-webpack-plugin": "^11.0.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "wasm-loader": "^1.3.0"
  }
}
